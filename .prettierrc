{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.ts",
        "options": {
          "parser": "typescript"
        }
      },
      {
        "files": "*.tsx",
        "options": {
          "parser": "typescript",
          "tsxBracketSameLine": false
        }
      }
    ]
  }
  